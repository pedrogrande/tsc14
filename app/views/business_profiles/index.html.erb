<%- model_class = BusinessProfile -%>
<div class="page-header">
  <h1><%=t '.title', :default => BusinessProfile %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:owner) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:twitter) %></th>
      <th><%= model_class.human_attribute_name(:facebook) %></th>
      <th><%= model_class.human_attribute_name(:linkedin) %></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:google) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:building) %></th>
      <th><%= model_class.human_attribute_name(:city) %></th>
      <th><%= model_class.human_attribute_name(:state) %></th>
      <th><%= model_class.human_attribute_name(:postcode) %></th>
      <th><%= model_class.human_attribute_name(:country) %></th>
      <th><%= model_class.human_attribute_name(:logo) %></th>
      <th><%= model_class.human_attribute_name(:image) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
      <th><%= model_class.human_attribute_name(:latitude) %></th>
      <th><%= model_class.human_attribute_name(:longitude) %></th>
      <th><%= model_class.human_attribute_name(:slug) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @business_profiles.each do |business_profile| %>
      <tr>
        <td><%= business_profile.id %></td>
        <td><%= business_profile.owner %></td>
        <td><%= business_profile.name %></td>
        <td><%= business_profile.description %></td>
        <td><%= business_profile.twitter %></td>
        <td><%= business_profile.facebook %></td>
        <td><%= business_profile.linkedin %></td>
        <td><%= business_profile.url %></td>
        <td><%= business_profile.google %></td>
        <td><%= business_profile.address %></td>
        <td><%= business_profile.building %></td>
        <td><%= business_profile.city %></td>
        <td><%= business_profile.state %></td>
        <td><%= business_profile.postcode %></td>
        <td><%= business_profile.country %></td>
        <td><%= business_profile.logo %></td>
        <td><%= business_profile.image %></td>
        <td><%= business_profile.email %></td>
        <td><%= business_profile.phone %></td>
        <td><%= business_profile.latitude %></td>
        <td><%= business_profile.longitude %></td>
        <td><%= business_profile.slug %></td>
        <td><%=l business_profile.created_at, format: :long %></td>
        <td>
          <%= link_to business_profile_path(business_profile), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to business_profile_path(business_profile), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_business_profile_path,
            :class => 'btn btn-primary' %>

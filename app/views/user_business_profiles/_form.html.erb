<%= simple_form_for(@user_business_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :business_profile %>
    <%= f.association :user %>
    <%= f.input :is_owner %>
    <%= f.input :position %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

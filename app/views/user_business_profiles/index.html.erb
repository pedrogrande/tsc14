<h1>Listing user_business_profiles</h1>

<table>
  <thead>
    <tr>
      <th>Business profile</th>
      <th>User</th>
      <th>Is owner</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_business_profiles.each do |user_business_profile| %>
      <tr>
        <td><%= user_business_profile.business_profile %></td>
        <td><%= user_business_profile.user %></td>
        <td><%= user_business_profile.is_owner %></td>
        <td><%= user_business_profile.position %></td>
        <td><%= link_to 'Show', user_business_profile %></td>
        <td><%= link_to 'Edit', edit_user_business_profile_path(user_business_profile) %></td>
        <td><%= link_to 'Destroy', user_business_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User business profile', new_user_business_profile_path %>

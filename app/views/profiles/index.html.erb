<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Tagline</th>
      <th>About</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Url</th>
      <th>Twitter</th>
      <th>Facebook</th>
      <th>Linkedin</th>
      <th>Google</th>
      <th>Image</th>
      <th>Slug</th>
      <th>Name</th>
      <th>Mentor</th>
      <th>Github</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user %></td>
        <td><%= profile.tagline %></td>
        <td><%= profile.about %></td>
        <td><%= profile.email %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.url %></td>
        <td><%= profile.twitter %></td>
        <td><%= profile.facebook %></td>
        <td><%= profile.linkedin %></td>
        <td><%= profile.google %></td>
        <td><%= profile.image %></td>
        <td><%= profile.slug %></td>
        <td><%= profile.name %></td>
        <td><%= profile.mentor %></td>
        <td><%= profile.github %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>

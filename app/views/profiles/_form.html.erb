<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :tagline %>
    <%= f.input :about %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :url %>
    <%= f.input :twitter %>
    <%= f.input :facebook %>
    <%= f.input :linkedin %>
    <%= f.input :google %>
    <%= f.input :image %>
    <%= f.input :slug %>
    <%= f.input :name %>
    <%= f.input :mentor %>
    <%= f.input :github %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
